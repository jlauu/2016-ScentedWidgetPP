{
  "manifest_version": 2,
  "omnibox": {"keyword": "SWPP"},
  "name": "ScentedWidgetPP",
  "description": "Web session modelling and analysis",
  "version": "1.0",
  "background": {
      "persistent": true,
      "scripts": [
          "lib/URI.js",
          "utils.js",
          "PageVisit.js",
          "ServerConnection.js",
          "BrowsingGraph.js",
          "UserCluster.js",
          "ClusterManager.js",
          "SessionManager.js",
          "omnibox.js",
          "main.js"
      ]
  },
  "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "lib/URI.js",
            "utils.js",
            "lib/jquery-1.12.1.min.js",
            "sniffer.js",
            "domcapture.js"
        ]
      }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ScentedWidgetPP"
  },
  "commands": {
      "_execute_browser_action": {
          "suggested_key": {
              "windows": "Ctrl+Shift+G",
              "mac": "Ctrl+Shift+G",
              "chromeos": "Ctrl+Shift+G",
              "linux": "Ctrl+Shift+G"
            },
            "description": "Open swpp minimap"
        }
    },
  "permissions": [
    "https://ajax.googleapis.com/",
    "https://swpp-server-stage.herokuapp.com/*",
    "storage",
    "identity",
    "identity.email",
    "background",
    "history",
    "tabs",
    "gcm"
  ]
}
